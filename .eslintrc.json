{
  "extends": ["next/core-web-vitals", "prettier"],
  "rules": {
    "no-console": "error",
    "no-unused-vars": "error",
    "no-debugger": "error",
    "react/jsx-no-target-blank": "off",
    "max-lines": [
      "error",
      { "max": 500, "skipBlankLines": true, "skipComments": true }
    ]
  }
}
